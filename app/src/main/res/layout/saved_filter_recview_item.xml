<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="wrap_content">

    <TextView
            android:text="From"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/date_from_title_textview"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/region_title_textview"
            app:layout_constraintStart_toStartOf="@+id/region_title_textview" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/date_from_textview"
            app:layout_constraintTop_toTopOf="@+id/date_from_title_textview"
            app:layout_constraintBottom_toBottomOf="@+id/date_from_title_textview"
            app:layout_constraintStart_toEndOf="@+id/date_from_title_textview" android:layout_marginStart="8dp"/>
    <TextView
            android:text="To"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/date_to__title_textview"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/product_title_textview"
            app:layout_constraintStart_toStartOf="@+id/product_title_textview"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/date_to_textview"
            app:layout_constraintTop_toTopOf="@+id/date_to__title_textview"
            app:layout_constraintBottom_toBottomOf="@+id/date_to__title_textview"
            app:layout_constraintStart_toEndOf="@+id/date_to__title_textview" android:layout_marginStart="8dp"/>
    <TextView
            android:text="Language"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/language_title_textview"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/ssessment_title_textview"
            app:layout_constraintStart_toStartOf="@+id/ssessment_title_textview"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/language_textview"
            app:layout_constraintTop_toTopOf="@+id/language_title_textview"
            app:layout_constraintBottom_toBottomOf="@+id/language_title_textview"
            app:layout_constraintStart_toEndOf="@+id/language_title_textview" android:layout_marginStart="8dp"/>
    <TextView
            android:text="@string/ssessment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/ssessment_title_textview"
            app:layout_constraintTop_toTopOf="@+id/product_title_textview"
            app:layout_constraintBottom_toBottomOf="@+id/product_title_textview"
            app:layout_constraintStart_toEndOf="@+id/product_textview" android:layout_marginStart="32dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/ssessment_textview"
            app:layout_constraintTop_toTopOf="@+id/ssessment_title_textview"
            app:layout_constraintBottom_toBottomOf="@+id/ssessment_title_textview"
            app:layout_constraintVertical_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/ssessment_title_textview" android:layout_marginStart="8dp"/>
    <TextView
            android:text="@string/product"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/product_title_textview"
            app:layout_constraintTop_toTopOf="@+id/region_textview"
            app:layout_constraintBottom_toBottomOf="@+id/region_textview"
            app:layout_constraintVertical_bias="1.0" app:layout_constraintStart_toEndOf="@+id/region_textview"
            android:layout_marginStart="32dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/product_textview"
            app:layout_constraintBottom_toBottomOf="@+id/product_title_textview"
            app:layout_constraintStart_toEndOf="@+id/product_title_textview" android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="@+id/product_title_textview"
    />
    <TextView
            android:text="Title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/filter_title_textview"
            tools:text="@tools:sample/lorem" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"/>
    <TextView
            android:text="@string/region"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/region_title_textview" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/filter_title_textview"
            app:layout_constraintStart_toStartOf="@+id/filter_title_textview"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/region_textview" app:layout_constraintStart_toEndOf="@+id/region_title_textview"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="@+id/region_title_textview"
            app:layout_constraintBottom_toBottomOf="@+id/region_title_textview"
            app:layout_constraintVertical_bias="1.0"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/delete_filter_textview"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="16dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>